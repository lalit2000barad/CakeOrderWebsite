<body>
    <nav class="navbar navbar-expand-sm" style="background-color: rgb(241, 154, 14);padding: 1px;">

        <div class="container-fluid">
          <img src="Vanilla-1s-269px.gif" alt="Italian Trulli" class="rounded" style="width: 50px;">
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#collapsibleNavbar">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-end" id="collapsibleNavbar">
            <ul class="navbar-nav">
              <li class="nav-item">
                <a class="nav-link " href="/">
                  <h9>Home</h9>
                </a>
              </li>
       
            
              <li class="nav-item">
                <a class="nav-link" href="/admin_login">
                  <h9>Login as Admin</h9>
                </a>
              </li>
             
            </ul>
          </div>
        </div>
    
      </nav>

    <div class="container" style="background-image: url(https://st3.depositphotos.com/8687452/15803/i/450/depositphotos_158035108-stock-photo-happy-birthday-cake.jpg);">
    <!--<div class="container" style="background-image:url();"-->
        <div class="row text-center ">
            <div class="col-md-12">
                <br /><br />
                <h2 style="color: orange;font-family: 'Times New Roman', Times, serif;">User Log In</h2>

                <h5>( Enter correct details to get access )</h5>
                <br />
            </div>
        </div>
        <div class="row ">

            <div class="col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3 col-xs-10 col-xs-offset-1">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <strong style="color:blue;"> Enter Details To Login </strong>
                    </div>
                    <div class="panel-body" style="background: rgba(240, 135, 50, 0.1);">

                        <form role="form" action="/Userlogin" method="POST">
                            <br>
                            <div class="form-group input-group">
                                <span class="input-group-addon"><i class="fa fa-envelope"></i></span>

                                <input type="text" class="form-control" name="Name"
                                    placeholder="Enter your User name. . . "  autofocus="autofocus"  required>
                            </div>
                            
   
                            <br />
                            <div class="form-group input-group">
                                <span class="input-group-addon"><i class="fa fa-envelope"></i></span>

                                <input type="text" class="form-control" name="email"
                                    placeholder="Enter your User Email. . . "  autofocus="autofocus" id="unique" required>
                            </div>
                            <div class="form-group input-group">
                                <span class="input-group-addon"><i class="fa fa-lock"></i></span>
                                <input type="password" class="form-control" name="psd"
                                    placeholder="Enter your Password. . . . ." required>
                            </div>
                            <div class="form-group">
                                <label class="checkbox-inline">
                                    <input type="checkbox" /> Remember me
                                </label>
                                <span class="pull-right">
                                    <a href="#">Forget password ? </a>
                                </span>
                            </div>

                           <!-- <a href="#" class="btn btn-primary " style="margin-left: 125px; background-color: orange;">Login Now</a>-->
                            <input type="submit" id="copy-button" class="btn btn-primary" onclick="check(this.form)" value="Login" >
                            <hr />
                            Not register ? <a href="register">click here </a>
                        </form>
                    </div>

                </div>
            </div>


        </div>
    </div>
    <script language="javascript">
    
       
          </script>
    <!-- SCRIPTS -AT THE BOTOM TO REDUCE THE LOAD TIME-->
    <!-- JQUERY SCRIPTS -->
    <script src="assets/js/jquery-1.10.2.js"></script>
    <!-- BOOTSTRAP SCRIPTS -->
    <script src="assets/js/bootstrap.min.js"></script>
    <!-- METISMENU SCRIPTS -->
    <script src="assets/js/jquery.metisMenu.js"></script>
    <!-- CUSTOM SCRIPTS -->
    <script src="assets/js/custom.js"></script>
    <script>
      document.getElementById("copy-button").addEventListener("click", function() {
        var inputText = document.getElementById("unique").value;
        localStorage.setItem("copiedText", inputText);
       
      });
    </script>
</body>

